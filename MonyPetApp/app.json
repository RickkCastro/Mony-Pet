{
  "expo": {
    "name": "MonyPet",
    "slug": "MonyPet",
    "version": "1.0.0.4",
    "assetBundlePatterns": [
      "**/*"
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "extra": {
      "eas": {
        "projectId": "ef1db521-85ba-4bcb-bbb4-614ae7683a6e"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.GreenCubesDeveloper.MonyPet"
    },
    "plugins": [
      [
        "onesignal-expo-plugin",
        {
          "mode": "development"
        }
      ]
    ],
    "android": {
      "package": "com.GreenCubesDeveloper.MonyPet",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#9690cc"
      },
      "icon": "./assets/icon.png",
      "versionCode": 2
    }
  }
}
